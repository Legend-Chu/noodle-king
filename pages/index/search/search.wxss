/* pages/index/search/search.wxss */
/* 搜索框样式 */
.search-wrap{
  margin: 0;
  padding: 20rpx;
  background-color: #ff6146;
}
.search-box{
  background-color: #fff;
  display: flex;
  border: 1rpx solid #dfdede;
  margin: 0 20rpx;
  border-radius: 20rpx;
  flex-direction: row;
  justify-content: space-between;
  padding: 10rpx 25rpx;
}
.search-image{
  width: 40rpx;
  height: auto;
}
.input-cell{
  width: 500rpx;
}

/* 商品列表 */
.good-box{
  display: flex;
  flex-direction: row;
  padding: 20rpx 20rpx;
  border: 4rpx solid rgb(231, 231, 231)
}
.good-img{
  height: 150rpx;
  width: 150rpx;
}
.title{
  font-size: 40rpx;
}
.box-left{
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.right{
  display: flex;
  width:600rpx;
  justify-content: space-between;

}
.soldMount{
  font-size: 38rpx;
  color: #cacaca
}
.price{
  font-size: 45rpx;
  line-height: 150rpx;
  margin-right: 20rpx;
  color: #ff6146;
}